       }
    },
    "dart": {
      "command": "dart_language_server",
      "args": [],
      "filetypes": ["dart"],
      "initializationOptions": {},
      "settings": {
        "dart": {
          "validation": {},
          "completion": {}
        }
      }
    },
    "kotlin" : {
      "command": "/home/nlaundry/.vim/plugged/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "filetypes" : ["kotlin", "kt"]
    }
  }
}
